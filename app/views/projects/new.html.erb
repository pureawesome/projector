<h1>Create a project</h1>

<%= form_for @project do |f| %>
  <fieldset class="field form-group">
    <%= f.label :name %>
    <%= f.text_field :name, :class => "form-control" %>
  </fieldset>
  <fieldset class="field form-group">
    <%= f.label :description %>
    <%= f.text_field :description, :class => "form-control" %>
  </fieldset>
  <fieldset class="field form-group">
    <%= f.label :start_date %>
    <%= f.date_field :start_date, :class => "form-control" %>
  </fieldset>
  <fieldset class="field form-group">
    <%= f.label :end_date_projected %>
    <%= f.date_field :end_date_projected, :class => "form-control" %>
  </fieldset>
  <fieldset class="field form-group">
    <%= f.label :end_date_actual %>
    <%= f.date_field :end_date_actual, :class => "form-control" %>
  </fieldset>
  <fieldset class="field form-group">
    <%= f.label :budget %>
    <%= f.number_field :budget, :class => "form-control" %>
  </fieldset>
  <fieldset class="field form-group">
    <%= f.label :cost %>
    <%= f.text_field :cost, :class => "form-control" %>
  </fieldset>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
