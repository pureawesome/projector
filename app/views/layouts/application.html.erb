<!DOCTYPE html>
<html>
<head>
  <title>Projector</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'layouts/messages' %>

  <div id="main-container" class="container-fluid">
    <nav class="col-xs-12">
      <%= render "menu" %>
    </nav>

    <div id="main-content" class="col-xs-12">
      <%= yield %>
    </div>
  </div>

</body>
</html>
